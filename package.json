{
  "name": "ljd-16-bit-computer",
  "version": "0.0.1",
  "description": "LJD 16-bit computer in typescript for node",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint --fix *.json 'src/**/*.ts'",
    "tsc": "tsc",
    "start": "ts-node src/ljd_16_bit_computer.ts",
    "doc": "typedoc --excludePrivate --out docs/ src && touch docs/.nojekyll",
    "all": "npm run lint && tsc && npm test"
  },
  "keywords": [
    "cpu",
    "16-bit",
    "emulator"
  ],
  "author": "Jonathan Di Trapani",
  "license": "ISC",
  "repository": "https://github.com/lj-ditrapani/16-bit-computer-ts",
  "dependencies": {
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "ljd-16-bit-cpu": "^0.3.5",
    "term-grid-ui": "^0.6.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  }
}
